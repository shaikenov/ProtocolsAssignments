@model IEnumerable<App.Models.Protocol>
    @using App.Models


@{
    ViewBag.Title = "LoggedIn";
}

@if (Session["UserID"] != null)
{

    <h2>userProtocols</h2>

    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ProtID)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Organization)
            </th>
            
            <th>
                @Html.DisplayNameFor(model => model.Title)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DateOfSubmission)
            </th>

            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            if (item.UserAccounts.Contains(ViewBag.usr))
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.ProtID)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Organization.Title)
                    </td>
                    
                    <td>
                        @Html.DisplayFor(modelItem => item.Title)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.DateOfSubmission)
                    </td>
                </tr>
            }

        }

    </table>
}


@model App.Models.Protocol

@{
    ViewBag.Title = "CreateProtocol";
}

<h2>CreateProtocol</h2>

@using (Html.BeginForm())
{
    <p>
        ID <br />
        @Html.EditorFor(model => model.ProtID)
    </p>

    <p>
        Title <br />
        @Html.EditorFor(model => model.Title)
    </p>

    <p>
        Organization <br />
        @Html.DropDownListFor(model => model.OrganizationID, ViewBag.Organizations as SelectList)
    </p>

    <p>
        Date <br />
        @Html.EditorFor(model => model.DateOfSubmission)
    </p>

    <p>
        Responsible <br />
        @Html.DropDownListFor(model => model.ResponsibleID, ViewBag.Responsibles as SelectList)
    </p>

    <p>
        <input type="submit" value="Save" />
    </p>

}

<div>
    @Html.ActionLink("Back", "ProtocolsList")
</div>


